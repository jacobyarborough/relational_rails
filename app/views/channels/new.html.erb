<%= form_with url: "/channels", method: :post, local: true do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name %><br>
    <%= form.label :cost %>
    <%= form.number_field :cost %><br>
    <%= form.radio_button :spanish, "yes" %>
    <%= form.label :spanish_yes, "Available in Spanish" %>
    <%= form.radio_button :spanish, "no" %>
    <%= form.label :spanish_no, "Unavailable in Spanish" %><br>
    <%= form.submit 'Create Channel' %>
<% end %>