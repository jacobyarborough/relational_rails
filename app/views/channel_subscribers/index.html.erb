<% @subscribers.each do |subscriber| %>
    <p><%= link_to subscriber.name, "/subscribers/#{subscriber.id}" %>  | <%= link_to "Edit #{subscriber.name}", "/subscribers/#{subscriber.id}/edit" %></p>
    <p>
    Age: <%= subscriber.age %><br>
    Top Market: <%= subscriber.top_market %>
    </p>
<% end %>
<h4><%= link_to "Create Subscriber", "/channels/#{params[:channel_id]}/subscribers/new" %></h4>